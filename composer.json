{
    "name": "silasjoisten/sonata-multiupload-bundle",
    "description": "A Bundle which allows you to enable Multiupload in Sonata",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Silas Joisten",
            "email": "silasjoisten@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "oskarstark/symfony-http-responder": "^1.0.0",
        "sonata-project/admin-bundle": "^4.2",
        "sonata-project/media-bundle": "^4.1",
        "symfony/twig-bridge": "^7.0",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.66",
        "matthiasnoback/symfony-dependency-injection-test": "^5.0",
        "phpunit/phpunit": "^9.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/webpack-encore-bundle": "^v2.0",
        "webmozart/assert": "^1.11"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SilasJoisten\\Sonata\\MultiUploadBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SilasJoisten\\Sonata\\MultiUploadBundle\\Tests\\": "tests/"
        }
    }
}
